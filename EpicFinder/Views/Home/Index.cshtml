    #step {
	   margin: 10px 0 60px;
	   padding: 0;
	   border:1px solid #a1a1a1;
	   background-color: #ffffff;
	   border-radius:10px;
        -moz-border-radius:10px; /* Firefox 3.6 and earlier */
