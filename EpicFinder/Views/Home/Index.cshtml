    #step li {
	    padding: 10px 0 6px 70px;
	    margin: 5px 0 5px;
	    list-style: none;
	    border-top: solid 1px #ccc;
	    position: relative;
    }
    #step h3 {
	    font: bold 200%/100% Arial, Helvetica, sans-serif;
	    margin: 0 0 5px;
	    color: #666;
